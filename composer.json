{
    "name": "neko-api/entity",
    "type": "library",
    "description": "NekoAPI Entity Component",
    "keywords": ["entity", "neko-api"],
    "homepage": "https://github.com/neko-api/entity",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": { "NekoAPI\\Component\\Entity\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "NekoAPI\\Component\\Entity\\Test\\": "tests" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
